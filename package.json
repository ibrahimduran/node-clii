  "bugs": {
    "url": "https://github.com/ibrahimeymenduran/node-clii/issues"
  },
  "homepage": "https://github.com/ibrahimeymenduran/node-clii#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0"
  }
}
